{
    "name": "rock",
    "displayName": "rock",
    "description": "Base support for Rock workspaces",
    "version": "0.0.1",
    "publisher": "rock-robotics",
    "engines": {
        "vscode": "^1.18.0"
    },
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
        ],
        "languages": [
            {
                "id": "ruby",
                "extensions": [".orogen", ".autobuild"]
            },
            {
                "id": "yaml",
                "extensions": [".osdeps"],
                "filenames": ["manifest"]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "js-yaml": "latest"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "fs-temp": "latest",
        "should": "latest",
        "@types/js-yaml": "latest",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "@types/should": "latest"
    }
}
